<file path="packages/config/nginx.conf">
# nginx.conf - Konfigureret til Docker-frit lokalt setup

# Grundlggende event-blok (kan vre tom for simple konfigurationer)
events {
    # worker_connections 1024; # Eksempel p en standardindstilling
}

# HTTP-blokken definerer servere og hvordan requests hndteres
http {

    # Definerer en server, der lytter p port 80 (standard HTTP port)
    server {
        listen 80;
        server_name localhost; # Serveren svarer p requests til localhost

        # Location block for API-kald (alt under /api/)
        location /api/ {
            # Stter headers for at proxy'en fungerer korrekt
            proxy_set_header Host $host;
            proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;

            # Viderestiller requests til NestJS API'en, der krer lokalt p port 5002
            # ndret fra host.docker.internal til localhost
            proxy_pass http://localhost:5002/;

            # Ndvendigt for WebSockets, hvis din API bruger dem
            proxy_http_version 1.1;
            proxy_set_header Upgrade $http_upgrade;
            proxy_set_header Connection "upgrade";
        }

        # Location block for Next.js Hot Module Replacement (HMR) WebSockets
        # Dette er specifikt for Next.js udviklingsserverens live-reloading funktionalitet
        location /_next/webpack-hmr {
            proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
            proxy_set_header Host $host;

            # Viderestiller til Next.js dev serveren, der krer lokalt p port 3000
            # ndret fra host.docker.internal til localhost
            proxy_pass http://localhost:3000;

            # Ndvendigt for WebSockets
            proxy_http_version 1.1;
            proxy_set_header Connection "upgrade";
            proxy_set_header Upgrade $http_upgrade;
        }

        # Location block for alle andre requests (rod-path og alt andet)
        # Disse vil blive hndteret af Next.js web-applikationen
        location / {
            # Viderestiller til Next.js appen, der krer lokalt p port 3000
            proxy_pass http://localhost:3000;

            # Standard headers for proxying
            proxy_set_header Host $host;
            proxy_set_header X-Real-IP $remote_addr;
            proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
            proxy_set_header X-Forwarded-Proto $scheme;
        }
    }
}
