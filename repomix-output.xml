  // men det er bedst at sikre, at den altid er sat.
}

export const api = createApi({
  reducerPath: "baseApi",
  baseQuery: fetchBaseQuery({
    // Brug den hentede baseUrl.
    // Hvis baseUrl er undefined (ikke sat), vil kald relativt til nuvrende host:port (f.eks. localhost:3003/hello)
    // hvilket er forkert, da API'et krer p localhost:5002.
    baseUrl: baseUrl || "/api", // Fallback til /api hvis den ikke er sat, men det br den vre.
  }),
  tagTypes: [],
  endpoints: (builder) => ({
    // Endpoint for at hente "Hello World" fra API'ets rod (/)
    // API'en er p http://localhost:5002/api, s query-URL'en skal vre relativ til det.
    // Hvis baseUrl er "http://localhost:5002/api", s vil query: "/" kalde "http://localhost:5002/api/"
    hello: builder.query<{ message: string }, void>({
      query: () => ({
        url: "/", // Dette vil blive tilfjet til baseUrl, f.eks. http://localhost:5002/api/
      }),
    }),
  }),
});

export const { useHelloQuery } = api;
