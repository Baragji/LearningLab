{
  "name": "turborepo-basic-shared",
  "version": "0.0.0",
  "private": true,
  "packageManager": "yarn@4.9.1",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "build:api": "turbo run build --filter=api...",
    "ensure-db": "chmod +x ./scripts/ensure-db.sh && ./scripts/ensure-db.sh",
    "dev": "yarn ensure-db && turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prisma:migrate": "yarn ensure-db && ./scripts/prisma-commands.sh migrate",
    "prisma:generate": "./scripts/prisma-commands.sh generate",
    "prisma:studio": "yarn ensure-db && ./scripts/prisma-commands.sh studio",
    "seed": "yarn ensure-db && cd apps/api && yarn seed"
  },
  "devDependencies": {
    "@nestjs/common": "^11.1.1",
    "@types/eslint": "^9.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.21",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint": "^9.27.0",
    "prettier": "^3.0.0",
    "prisma": "6.8.2",
    "rxjs": "^7.8.2",
    "turbo": "^2.3.5",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@nestjs/config": "^4.0.2",
    "@radix-ui/react-slot": "^1.2.3",
    "@types/axios": "^0.14.4",
    "@types/passport-github2": "^1.2.9",
    "@types/passport-google-oauth20": "^2.0.16",
    "axios": "^1.9.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "reflect-metadata": "^0.2.2"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=22 <23"
  },
  "resolutions": {
    "@types/react": "18.2.18",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.3.3"
  }
}
