{
    "exclude": [
      "**/node_modules/**",
      "**/dist/**",
      "**/build/**",
      "**/.git/**",
      "**/.next/**",
      "**/.turbo/**",
      "**/coverage/**"
    ],
    "watch": [
      "apps",
      "packages",
      "scripts",
      "prisma"
    ],
    "typeScript": {
      "incremental": true,
      "enableQuickFixMode": true,
      "fullTypeCheckOnSave": false
    }
  }